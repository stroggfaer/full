{
  "name": "fullstack",
  "version": "1.0.0",
  "description": "frontend + backend",
  "main": "index.js",
  "engines": {
    "node": "10.16.0",
    "npm": "6.11.0"
  },
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
  },
  "keywords": [
    "express",
    "angular"
  ],
  "author": "Ruslan",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.29.4",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.7.0",
    "sequelize": "^5.19.0",
    "sequelize-models-generator": "0.0.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.3"
  }
}
